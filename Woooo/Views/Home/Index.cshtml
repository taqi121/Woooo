<div class="sign-up">
    <div class="layer"> </div>
    <!-- end layer -->
    <div class="inner">
        @using (Html.BeginForm("SignUp", "MainCredentials", FormMethod.Post))
        {
            @Html.AntiForgeryToken()

            <div class="form-horizontal login-box">
                <h2>Sign Up</h2>
                <hr />
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div class="form-group user-box">
                    <div class="col-md-10" style="margin-top:10px;">
                        <input type="text" name="FirstName" id="FirstName" class="form-control" placeholder="FirstName..." />
                    </div>
                </div>

                <div class="form-group user-box">
                    <div class="col-md-10">
                        <input type="text" name="LastName" id="LastName" class="form-control" placeholder="LastName..." />
                    </div>
                </div>

                <div class="form-group user-box">
                    <div class="col-md-10">
                        <input type="text" name="Email" id="Email" class="form-control" placeholder="Email..." />
                    </div>
                </div>

                <div class="form-group user-box">
                    <div class="col-md-10">
                        <input type="password" name="Pass" id="Pass" class="form-control" placeholder="Password..." />

                    </div>
                </div>
                <div class="form-group user-box">
                    <div class="col-md-10">
                        <input type="password" name="CPassword" id="CPassword" class="form-control" placeholder="Confirm Password..." />

                    </div>
                </div>


                <div class="form-group user-box">
                    <button type="submit" class="btn btn-dark btnRegister" style="margin-left:32%;">Sign Up</button>
                </div>
            </div>
        }
    </div>
    <!-- end inner -->
</div>

<div class="sign-in">
    <div class="layer"> </div>
    <!-- end layer -->
    <div class="inner">
        @using (Html.BeginForm("SignIn", "MainCredentials", FormMethod.Post))
        {
            @Html.AntiForgeryToken()

            <div class="form-horizontal login-box">
                <h2>Sign In</h2>
                <hr />
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                <div class="form-group user-box">
                    <div class="col-md-10" style="margin-top:10px;">
                        <input type="text" name="userName" id="userName" class="form-control" placeholder="eg. john@example.com" />
                    </div>
                </div>

                <div class="form-group user-box">
                    <div class="col-md-10">
                        <input type="password" name="Password" id="InputPassword" class="form-control" placeholder="********" />
                        <span style="color:aliceblue; position:absolute; right:2px;transform:translate(0,50%);top:10%;cursor:pointer;">
                            <i class="fa fa-eye" aria-hidden="true" id="eye" onclick="ShowPWD();"></i>
                        </span>
                    </div>
                </div>
                <div class="form-group user-box">
                    <button type="submit" class="btn btn-dark btnRegister" style="margin-left:32%;">Login</button>
                </div>
            </div>
        }

    </div>
    <!-- end inner -->
</div>
